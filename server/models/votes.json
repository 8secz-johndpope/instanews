{
  "name": "votes",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": true,
  "properties": {
    "modelName": {
      "type": "string",
      "required": true
    },
    "rating": {
      "type": "number"
    },
    "commentRating": {
      "type": "number"
    },
    "clicksCount": {
      "type": "number"
    },
    "viewsCount": {
      "type": "number"
    },
    "version": {
      "type": "number"
    },
    "created": {
      "type": "date"
    },
    "modified": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "upVotes": {
      "type": "hasMany",
      "model": "upVote",
      "polymorphic": "votable"
    },
    "downVotes": {
      "type": "hasMany",
      "model": "downVote",
      "polymorphic": "votable"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
