{
  "name": "click",
  "base": "PersistedModel",
  "idInjection": true,
  "indexes": {
    "parent_index": {
      "clickableId": 1,
      "clickableType": 1
    }
  },
  "options": {
    "validateUpsert": true
  },
  "hidden": [
    "location",
    "viewId"
  ],
  "properties": {
    "created": {
      "type": "date"
    },
    "location": {
      "type": "geopoint"
    }
  },
  "validations": [],
  "relations": {
    "journalist": {
      "type": "belongsTo",
      "model": "journalist",
      "foreignKey": "username"
    },
    "clickable": {
      "type": "belongsTo",
      "polymorphic": true
    },
    "view": {
      "type": "belongsTo",
      "model": "view",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": []
}
