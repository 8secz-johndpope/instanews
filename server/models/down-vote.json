{
  "name": "downVote",
  "base": "click",
  "idInjection": true,
  "indexes": {
    "singleton_index": {
      "keys": {
        "username": 1,
        "clickableType": 1,
        "clickableId": 1
      },
      "options": {
        "unique": true
      }
    }
  },
  "properties": {},
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "__get__clickable"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": {}
}
