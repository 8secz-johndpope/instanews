{
  "name": "article",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "properties": {
    "isPrivate": {
      "type": "boolean",
      "required": true
    },
    "date": {
      "type": "date"
    },
    "votes": {
      "type": "votes"
    },
    "articleId": {
      "type": "number",
      "id": true,
      "required": true,
      "index": true
    },
    "location": {
      "type": "geopoint",
      "required": true,
      "index": true
    }
  },
  "validations": [],
  "relations": {
    "subarticles": {
      "type": "hasMany",
      "model": "subarticle",
      "foreignKey": "parentId"
    },
    "journalists": {
      "type": "hasMany",
      "model": "journalist",
      "foreignKey": "parentId",
      "through": "subarticle"
    }
  },
  "acls": [],
  "methods": []
}
